@article{Ganong1980,
author = {Ganong, W F},
title = {{Phonetic categorization in auditory word perception.}},
journal = {Journal of Experimental Psychology: Human Perception and Performance},
year = {1980},
volume = {6},
number = {1},
pages = {110--125},
annote = {- This article examines the influence of lexical status of a phonetic sequence (i.e., whether the phonetic sequence is a word) onphonetic categorization - We recognize a phoneme faster than if we hear it in a real word than in a nonword. - this real word word advantage reflects a perceptual bias in favor of words similar to the perceptual bias that favors high-frequency words over low-frequency words in noise (Phoneme restoration, Warren, 1970). - The purpose of this article is to determine the stage in the perceptual process at which the biasing effect of lexical status appears. - Does it follow phonetic categorization or does it happen before - ie. can it influence the interpretation of acoustic cues? (which underlies phonetic categorization)

