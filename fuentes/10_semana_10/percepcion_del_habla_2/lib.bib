local-url = {file://localhost/Users/casillas/Dropbox/Papers%20Documents/Papers2/Tarnovska/2004/Did%C3%A1ctica/Tarnovska2004Sobre_los_refranes_de_El_Quijote_The_proverbs_in_The_Quixote,Did%C3%A1ctica.pdf},
